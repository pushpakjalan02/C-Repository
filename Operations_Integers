/*6. Perform summation, subtraction, multiplication and division with integer numbers only.*/

#include <stdio.h>
int main()
{
	int a,b;
	printf("Enter nos. \n");
	scanf("%d",&a);
	scanf("%d",&b);
	printf("Sum is: %d \n",(a+b));
	printf("Difference is: %d \n",(a-b));
	printf("Multiplication is: %d \n", (a*b));
	printf("Division is: %d \n",(a/b));
	return 0;
}
